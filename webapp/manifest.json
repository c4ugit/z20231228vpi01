                "uri": "/sap/opu/odata/IWFND/CATALOGSERVICE;v=2/Annotations(TechnicalName='ZC4UVPI01_ANNO_MDL',Version='0001')/$value/",
                "type": "ODataAnnotation",
                "settings": {
                    "localUri": "localService/ZC4UVPI01_ANNO_MDL.xml"
                }
            }
        },
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.12.1",
            "toolsId": "a66aef8c-29d6-476e-a479-383b1d733722"
        }
    },
    "sap.ui": {
        "fullWidth": true,
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "dependencies": {
            "minUI5Version": "1.71.49",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.f": {},
                "sap.suite.ui.generic.template": {},
                "sap.ui.comp": {},
                "sap.ui.generic.app": {},
                "sap.ui.table": {},
                "sap.ushell": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "zmmvpi01.app.z20231228mmvpi01.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {}
            },
            "globalModelHelp": {
                "type": "sap.ui.model.json.JSONModel",
                "preload": true
            },
            "globalModelUserInfo": {
                "type": "sap.ui.model.json.JSONModel",
                "preload": true
            },
            "globalModelVendorInfo": {
                "type": "sap.ui.model.json.JSONModel",
                "preload": true
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.f.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "zmmvpi01.app.z20231228mmvpi01.view",
                "controlAggregation": "beginColumnPages",
                "controlId": "layout",
                "clearControlAggregation": false,
                "bypassed": {
					"target": "notFound"
				}				
            },
            "routes": [
                {
                    "name": "routeOverviewVendorInvoice",
                    "pattern": "",
                    "target": [
                        "TargetOverViewVendorInvoice"
                    ]
                },
                {
				"pattern": "DetailInvoice/{objectId}",
					"name": "routeDetailVendorInvoice",
					"target": [
						"TargetOverViewVendorInvoice",
						"TargetDetailVendorInvoice"
					]
				},
                {
				"pattern": "NewInvoice",
					"name": "routeCreateVendorInvoice",
					"target": [
						"TargetCreateVendorInvoice"					
					]
				}
            ],
            "targets": {
                "TargetOverViewVendorInvoice": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "OverViewVendorInvoice",
                    "viewName": "OverViewVendorInvoice",
                    "viewLevel": 1
                },
                "TargetDetailVendorInvoice": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "DetailVendorInvoice",
                    "viewName": "DetailVendorInvoice",
                    "controlAggregation": "midColumnPages",
                    "viewLevel": 1
                },
                "TargetCreateVendorInvoice": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "CreateVendorInvoice",
                    "viewName": "CreateVendorInvoice",                 
                    "viewLevel": 1
                }
            }
        },
        "rootView": {
            "viewName": "zmmvpi01.app.z20231228mmvpi01.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        }
    }
}